
<div class="wrapper">
  <div class="to_from"
    <%= form_for :welcome do |f| %>
      <p>
        <%= f.text_field :from, :placeholder => 'From' %>
        <%= f.text_field :from, :placeholder => 'From' %><br>
        <br><%= f.text_field :from, :placeholder => 'To' %><br>
        <%=f.label :date, "Date" %>
         <%=f.date_select :date, id: "dateField" %>
      </p>

      <p>
        <%= f.submit "Find Flights" %>
      </p>
    <% end %>
  </div><!-- to_from -->
</div><!-- wrapper -->





